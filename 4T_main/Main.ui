                            <property name="styleSheet">
                             <string notr="true">QScrollBar:vertical {
	border: 1px solid rgb(0, 192, 255);
    background: rgb(23, 37, 74);
    margin: 15px 0px 15px 0px;
/*	border-radius: 8px;*/
}

QScrollBar::handle:vertical {	
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 rgba(0, 85, 255, 255), stop:1 rgba(0, 192, 255, 255));
	border-radius: 4px;
}
QScrollBar::handle:vertical:hover{	
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 rgba(0, 0, 255, 255), stop:1 rgba(0, 85, 255, 255));
}
QScrollBar::handle:vertical:pressed {	
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 rgba(170, 0, 0, 255), stop:1 rgba(255, 60, 60, 255));
}

/* Up button */
QScrollBar::sub-line:vertical {
	border-image: url(:/LBlue_Icon/Icon_LBlue/chevron-up.svg);
	border: 1px solid  rgb(0, 180, 255);
	background-color: rgba(0, 0, 0, 0);
	height: 15px;
	border-radius: 2px;
	subcontrol-position: top;
	subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical:hover {	
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 rgba(0, 0, 255, 255), stop:1 rgba(0, 85, 255, 255));
}
QScrollBar::sub-line:vertical:pressed {	
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 rgba(170, 0, 0, 255), stop:1 rgba(255, 60, 60, 255));
}

/* Down button */
QScrollBar::add-line:vertical {
border-image: url(:/LBlue_Icon/Icon_LBlue/chevron-down.svg);
	border: 1px solid  rgb(0, 180, 255);
	background-color: rgba(0, 0, 0, 0);
	height: 15px;
	border-radius: 2px;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
}
QScrollBar::add-line:vertical:hover {	
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 rgba(0, 0, 255, 255), stop:1 rgba(0, 85, 255, 255));
}
QScrollBar::add-line:vertical:pressed {	
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 rgba(170, 0, 0, 255), stop:1 rgba(255, 60, 60, 255));
}

/* RESET ARROW */
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
	background: none;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}</string>
